./configure \
     --prefix=/root/frr \
     --includedir=/root/frr/include \
     --enable-exampledir=/root/frr/share/doc/frr/examples \
     --bindir=/root/frr/bin \
     --sbindir=/root/frr/lib/frr \
     --libdir=/root/frr/lib/frr \
     --libexecdir=/root/frr/lib/frr \
     --localstatedir=/root/frr/var/run/frr \
     --sysconfdir=/root/frr/etc/frr \
     --with-moduledir=/root/frr/lib/frr/modules \
     --with-libyang-pluginsdir=/root/frr/lib/frr/libyang_plugins \
     --enable-configfile-mask=0640 \
     --enable-logfile-mask=0640 \
     --enable-snmp=agentx \
     --enable-multipath=64 \
     --enable-user=root \
     --enable-group=root \
     --enable-dev-build \
     --with-pkg-git-version \
     --with-pkg-extra-version=-ubpf \
     --enable-systemd=yes \
     --disable-bgpd \
     --disable-ripd \
     --disable-ospfd \
     --disable-ospf6d \
     --disable-ldpd \
     --disable-nhrdp \
     --disable-eigrpd \
     --disable-babeld \
     --disable-isisd \
     --disable-pimd \
     --disable-pbrd \
     --disable-sharpd \
     --disable-bgp-announce \
     --disable-bgp-vnc \
     --disable-bgp-bmp \
     --disable-vtysh \
     UBPF_LIB=/home/vagrant/Master_thesis/frr/ubpf_tools \
     UBPF_INC=/home/vagrant/Master_thesis/frr/ubpf_tools/include

